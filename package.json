{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "serve": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.12.1",
    "@builder.io/qwik-city": "0.0.118",
    "@builder.io/qwik-react": "0.1.1",
    "@editorjs/image": "^2.6.2",
    "@editorjs/link-autocomplete": "^0.0.0",
    "@editorjs/simple-image": "^1.4.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@types/auth0": "^2.35.6",
    "@types/auth0-js": "^9.14.6",
    "@types/editorjs__header": "^2.6.0",
    "@types/eslint": "8.4.8",
    "@types/jsdom": "^20.0.0",
    "@types/node": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-plugin-qwik": "0.12.0",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4",
    "vite": "3.2.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.25.0",
    "@editorjs/header": "^2.6.2",
    "@editorjs/list": "^1.7.0",
    "arangojs": "^8.0.0",
    "auth0": "^2.44.0",
    "auth0-js": "^9.19.1",
    "cheerio": "^1.0.0-rc.12",
    "editorjs-hyperlink": "^1.0.6",
    "normalize.css": "^8.0.1",
    "slate": "^0.84.0",
    "slate-react": "^0.83.2"
  }
}
